  "dependencies": {
    "cheerio": "1.0.0-rc.3",
    "request": "2.88.0",
    "request-promise-native": "1.0.7"
  },
  "devDependencies": {
    "@semantic-release/exec": "3.3.2",
    "@semantic-release/git": "7.0.8",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
