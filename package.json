  "bugs": {
    "url": "https://github.com/jaypeng2015/aws-this-week/issues"
  },
  "homepage": "https://github.com/jaypeng2015/aws-this-week#readme",
  "dependencies": {
    "cheerio": "1.0.0-rc.2",
    "request": "2.85.0",
    "request-promise-native": "1.0.5"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-prettier": "2.6.0",
    "jest": "22.4.3",
    "prettier": "1.12.1",
