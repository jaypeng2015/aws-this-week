{
  "name": "aws-this-week",
  "version": "1.0.0",
  "description": "A cron lambda function that send `AWS This Week` YouTube video to a slack channel.",
  "scripts": {
    "deploy": "serverless deploy -v",
    "invoke-local": "serverless invoke local --function notify",
    "invoke": "serverless invoke --function notify",
    "lint": "eslint . --ext .js",
    "release": "semantic-release --debug",
    "remove": "serverless remove -v",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaypeng2015/aws-this-week.git"
  },
  "keywords": [
    "aws"
  ],
  "author": "Jingwei Peng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaypeng2015/aws-this-week/issues"
  },
  "homepage": "https://github.com/jaypeng2015/aws-this-week#readme",
  "dependencies": {
    "cheerio": "1.0.0-rc.2",
    "request": "2.87.0",
    "request-promise-native": "1.0.5"
  },
  "devDependencies": {
    "@semantic-release/exec": "^2.2.4",
    "@semantic-release/git": "6.0.1",
    "eslint": "5.1.0",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-prettier": "2.6.2",
    "jest": "23.4.0",
    "prettier": "1.13.7",
    "semantic-release": "15.7.1",
    "serverless": "1.28.0",
    "serverless-prune-plugin": "1.3.1"
  },
  "engines": {
    "node": "8.10.0",
    "npm": "5.6.0"
  }
